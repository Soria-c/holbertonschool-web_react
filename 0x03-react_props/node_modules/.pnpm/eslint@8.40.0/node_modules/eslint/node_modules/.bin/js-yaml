#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/enki/Documents/Holberton_FullStack_Spec/holbertonschool-web_react/0x02-react_intro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/enki/Documents/Holberton_FullStack_Spec/holbertonschool-web_react/0x02-react_intro/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
